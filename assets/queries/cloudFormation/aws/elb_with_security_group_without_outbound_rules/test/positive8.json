{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {
        "MySG": {
            "Type": "AWS::EC2::SecurityGroup",
            "Properties": {
                "GroupDescription": "SG with empty egress inline",
                "VpcId": "vpc-123456",
                "SecurityGroupEgress": []
            }
        },
        "MyClassicLB": {
            "Type": "AWS::ElasticLoadBalancing::LoadBalancer",
            "Properties": {
                "SecurityGroups": [
                    "MySG"
                ],
                "Listeners": [
                    {
                        "LoadBalancerPort": 80,
                        "InstancePort": 80,
                        "Protocol": "HTTP"
                    }
                ],
                "Subnets": [
                    "subnet-123"
                ]
            }
        }
    }
}